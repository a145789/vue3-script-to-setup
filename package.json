{
  "name": "vue3-script-to-setup",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.0",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vue3-script-to-setup.git"
  },
  "bugs": "https://github.com/antfu/vue3-script-to-setup/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "rome ci ./src",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release-one": "standard-version -r major",
    "release-two": "standard-version -r minor",
    "release-three": "standard-version -r patch"
  },
  "devDependencies": {
    "@antfu/ni": "^0.18.0",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "esno": "^0.16.3",
    "rome": "^10.0.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vite": "^3.2.3",
    "vitest": "^0.25.2",
    "standard-version": "^9.5.0"
  }
}
