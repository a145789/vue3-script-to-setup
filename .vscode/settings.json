{
  "prettier.enable": false,
  "eslint.enable": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.rome": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "files.eol": "\n"
}
